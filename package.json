{
  "name": "shopify-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "theme download && npm run git-push && npm run prod",
    "start:env": "theme download --config=custom_config.yml && npm run git-push && npm run prod",
    "start:env:live": "theme download --config=custom_config.yml && npm run git-push && npm run prod:live",
    "start:dev": "theme download && npm run git-push && npm run dev",
    "start:dev:env": "theme download --config=custom_config.yml && npm run git-push && npm run dev",
    "start:dev:env:live": "theme download --config=custom_config.yml && npm run git-push && npm run dev:live",
    "start:live": "theme download && npm run git-push && npm run prod:live",
    "start:dev:live": "theme download && npm run git-push && npm run dev:live",
    "dev": "npm-run-all --parallel \"webpack:dev\" \"shopify\"",
    "dev:live": "npm-run-all --parallel \"webpack:dev\" \"shopify:live\"",
    "dev:env": "npm-run-all --parallel \"webpack:dev-env\" \"shopify:env\"",
    "dev:env:live": "npm-run-all --parallel \"webpack:dev-env\" \"shopify:env:live\"",
    "prod": "npm-run-all --parallel \"webpack:prod\" \"shopify\"",
    "prod:live": "npm-run-all --parallel \"webpack:prod\" \"shopify:live\"",
    "prod:env": "npm-run-all --parallel \"webpack:prod-env\" \"shopify:env\"",
    "prod:env:live": "npm-run-all --parallel \"webpack:prod-env\" \"shopify:env:live\"",
    "env": "node ./config/env.js && npm run prod:env",
    "env:dev": "node ./config/env.js && npm run dev:env",
    "shopify": "theme watch --notify=./theme.update",
    "shopify:env": "theme watch --notify=./theme.update --config=custom_config.yml",
    "shopify:live": "theme watch --notify=./theme.update --allow-live",
    "shopify:env:live": "theme watch --notify=./theme.update --config=custom_config.yml --allow-live",
    "webpack:dev": "webpack --watch --config ./config/webpack.config.js --mode=development",
    "webpack:prod": "webpack --watch --config ./config/webpack.config.js --mode=production",
    "webpack:dev-env": "webpack --watch --config ./config/webpack.config.js --mode=development --env=true",
    "webpack:prod-env": "webpack --watch --config ./config/webpack.config.js --mode=production --env=true",
    "git-push": "git add . && git commit -m \"Add theme files\" && git push",
    "deploy": "webpack --config ./config/webpack.config.js --mode=production && theme deploy",
    "deploy:live": "webpack --config ./config/webpack.config.js --mode=production && theme deploy --allow-live",
    "deploy:env": "webpack --config ./config/webpack.config.js --mode=production && theme deploy --config=custom_config.yml",
    "deploy:env:live": "webpack --config ./config/webpack.config.js --mode=production && theme deploy --allow-live --config=custom_config.yml",
    "deploy:dev": "webpack --config ./config/webpack.config.js --mode=development && theme deploy",
    "deploy:dev:env": "webpack --config ./config/webpack.config.js --mode=development && theme deploy --config=custom_config.yml",
    "deploy:dev:live": "webpack --config ./config/webpack.config.js --mode=development && theme deploy --allow-live",
    "deploy:dev:env:live": "webpack --config ./config/webpack.config.js --mode=development && theme deploy --allow-live --config=custom_config.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^4.1.0",
    "colors": "^1.4.0",
    "css-loader": "^4.2.2",
    "file-loader": "^6.0.0",
    "inquirer": "^7.3.3",
    "jquery": "^3.5.1",
    "js-yaml": "^3.14.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "yaml": "^1.10.0",
    "yargs": "^15.4.1"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "slick-carousel": "^1.8.1"
  }
}
